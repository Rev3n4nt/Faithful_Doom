	override void Tick()
	{
		if ( CountInv("BEPowerTorch") )
		{
			if(fadeInTimer > 50) fadeInTimer = 50;
			else fadeInTimer+=0.1;
			Shader.SetUniform1f(player,"NightVisionShader","fadeInTimer",fadeInTimer);
			Shader.SetEnabled(player, "NightVisionShader", true);
		}
		else
		{
			fadeInTimer--;
			Shader.SetUniform1f(player,"NightVisionShader","fadeInTimer",fadeInTimer);
			if(fadeInTimer <= 0)
			{
				Shader.SetEnabled(player, "NightVisionShader", false);
				fadeInTimer = 0;
			}
			
		}
	
		int Armor = 0;
		let arm = BasicArmor(FindInventory("BasicArmor"));
		if (arm)	Armor = arm.Amount;
		
		if(BloodInTics > 0) BloodInTics --;
		else
		{
			if( startBloodAlpha > 0.0)
			{
				startBloodAlpha -= 0.01;
				Shader.SetUniform1f(player, "ScreenBloodOne", "EffectAlpha", startBloodAlpha);
				Shader.SetUniform1f(player, "ScreenBloodTwo", "EffectAlpha", startBloodAlpha);
			}
			else
			{
				Shader.SetEnabled(Player,"ScreenBloodOne",False);
				Shader.SetEnabled(Player,"ScreenBloodTwo",False);
			}
		}
		
		if(Armor >= 200) SetInventory("ReloadSpeed",2);
		else if (Armor >= 110) SetInventory("ReloadSpeed",1);
		else SetInventory("ReloadSpeed",0);
		
		if( Health < 26 && Health > 0)
		{
			//Console.Printf(String.Format("%i",Angle));
			double zerk = 0.35 - (health * 0.01);
			angle+=frandom(-zerk,zerk);pitch+=frandom(-zerk,zerk);
			Shader.SetEnabled(Player,"SaturationShader",true);
			Shader.SetEnabled(Player,"WobleShader",true);
		}
		else
		{
			Shader.SetEnabled(Player,"SaturationShader",false);
			Shader.SetEnabled(Player,"WobleShader",false);
		}

		if (damageSize > 0) {
			Shader.SetEnabled(player, "DamageShader", true);
			Shader.SetUniform1f(player, "DamageShader", "damageAmount", damageSize);
		}
		else Shader.SetEnabled(player, "DamageShader", false);
		
		if ( CountInv("HeatHazeTimer") != 0)
		{
			Shader.SetEnabled(player, "HeatHaze", true);
			A_TakeInventory("HeatHazeTimer",1);
		}
		else Shader.SetEnabled(player, "HeatHaze", false);		
		
		double returnSpeed = 0;
		if (damageSize != 0) returnSpeed = 0.01 ** 1/(damageSize * 5);
		if (returnSpeed < 0.01) returnSpeed = 0.01;
		
		damageSize -= returnSpeed;
		damageSize = clamp(damageSize, 0, 0.5);

		if( CountInv("GoFatality") == 1 )
		{
			A_Stop();
			A_Warp(AAPTR_DEFAULT,0,0,0,0,WARPF_NOCHECKPOSITION|WARPF_TOFLOOR);
		}
		if(player)
		{
		double fm = player.cmd.forwardmove;
		double sm = player.cmd.sidemove;

		if(player.cmd.buttons & BT_SPEED) PlayerRunOrWalk=1;
		else if( 6400<max( abs(fm),abs(sm) ) ) PlayerRunOrWalk=0;
		else PlayerRunOrWalk=-1;	
		}
		Super.Tick();
	
		// int fall = ( lastVel - vel).length();
		
		// if ( fallingPitch) A_SetPitch(pitch + frandom(4,6), SPF_INTERPOLATE);

		// if ( fall > 15 && ( Cvar.FindCvar("beberserkergameplaymode").GetInt() == 0 ||
							// CountInv("PowerStrength") == 0 ) && (player.WeaponState & WF_WEAPONREADY))
		// {
			// fallingPitch = true;
			// int size = weaponList.size();
			// for (int i = 0; i < size; i++)
			// {
				// if( self.player.ReadyWeapon is "FallingHands") break;
				// if( self.player.ReadyWeapon is weaponList[i])
