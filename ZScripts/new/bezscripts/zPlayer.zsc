					// lastWeapon = weaponList[i];
					// break;
				// }
			// }
			// A_PlaySound("Player/HeavyFallingSound");
			// A_SelectWeapon("FallingHands");
		// }
	
		int timer = level.time % 60;
		
		if ( !(timer % 60) )
		{
			if ( CountInv("Cacodemonia") )
			{
				A_TakeInventory("Cacodemonia",1);
			}
		}
		
		
		
